{
  "name": "uif-fabric",
  "version": "0.0.0",
  "license": "MIT",
  "main": "uif-fabric.umd.js",
  "jsnext:main": "uif-fabric.esm.js",
  "module": "uif-fabric.esm.js",
  "types": "uif-fabric.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run bundle & node tools/build-defn.js & ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prebundle": "rimraf dist",
    "postbundle": "node tools/package-copy.js & node tools/package-cleanup.js & node tools/package-removecode.js",
    "bundle-tsc": "tsc -p tools/tsconfig-build.json",
    "bundle-ngc": "ngc -p tools/tsconfig-build.json",
    "bundle-rollup":"rollup -c tools/rollup.config.umd.js & rollup -c tools/rollup.config.esm.js",
    "bundle": "npm run prebundle & rollup -c tools/rollup.config.umd.js & rollup -c tools/rollup.config.esm.js && tsc -p tools/tsconfig-build.json && ngc -p tools/tsconfig-build.json & npm run postbundle",
    "publish": "npm run build & node tools/deploy.js --host=$AZURE_WA_SITE --user=$AZURE_WA_USERNAME --password=$AZURE_WA_PASSWORD"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/router": "^5.1.0",
    "core-js": "^2.4.1",
    "office-ui-fabric-core": "9.3.1",
    "office-ui-fabric-js": "1.5.0",
    "prismjs": "1.10.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.4",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/language-service": "^5.1.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "del": "^3.0.0",
    "ftp-deploy": "^1.2.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsftp": "^2.1.3",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.7.2",
    "prismjs": "1.10.0",
    "protractor": "~5.1.2",
    "rollup": "^0.56.3",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-typescript": "^0.8.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
